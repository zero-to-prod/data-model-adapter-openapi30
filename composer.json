{
  "type": "library",
  "name": "zero-to-prod/data-model-adapter-openapi30",
  "description": "An adapter for the OpenAPI 3.0 for DataModelGenerator",
  "keywords": [
    "zero-to-prod",
    "DataModelAdapterOpenApi30",
    "data-model-adapter-openapi30"
  ],
  "homepage": "https://github.com/zero-to-prod/data-model-adapter-openapi30",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "zero-to-prod/data-model-generator": "^0.1.2",
    "zero-to-prod/data-model": "^81.6",
    "zero-to-prod/transformable": "^71.0",
    "zero-to-prod/data-model-helper": "^81.5",
    "zero-to-prod/data-model-openapi30": "^0.1.1",
    "zero-to-prod/psr4-classname": "^71.0"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\DataModelAdapterOpenapi30\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Factories\\": "factories/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "suggest": {
    "zero-to-prod/data-model": "Transform Data to Type-Safe DTOs",
    "zero-to-prod/data-model-helper": "Helpers for a DataModel."
  }
}
